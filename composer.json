{
    "name": "tohmua/consolidare",
    "description": "Consolidare merge tool",
    "keywords": ["merge", "merging", "merge tool", "record merging"],
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpspec/prophecy": "^1.6",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Consolidare\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --coverage-html='tests/_output' --coverage-text='php://stdout' --colors='auto' --whitelist src tests"
        ]
    }
}
