{
    "name": "Tohmua/Consolidare",
    "description": "Consolidare merge tool",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "phpspec/prophecy": "^1.6",
        "monolog/monolog": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "RecordMerge\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --coverage-html tests/_output --whitelist src tests"
        ]
    }
}
